<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!--model.person.Persona root -->
	<class name="model.parte.Historico" table="historico">
		 <id name="id" type="long" column="id">
			<generator class="increment"/>
		</id>
	    <property name="empleado" column="empleado" type="string"  />
	    <property name="fecha" column="fecha" type="date"/>
            <property name="estado" column="estado" type="string"/>
	</class>
</hibernate-mapping>
