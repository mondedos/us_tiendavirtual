<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.DatosAcceso" table="DatosAcceso">
        <id name="datosAcceso_id" column="acceso_id">
            <generator class="increment"/>
        </id>
        
        <property name="ultimo" type="date"/>
        <property name="anterior" type="date"/>
        <property name="fechaAlta" type="date"/>
        <property name="numConexiones" type="int"/>
       
    </class>
    

</hibernate-mapping>